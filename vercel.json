{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@2.0.1"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9"
    }
} 