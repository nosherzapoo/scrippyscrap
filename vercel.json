{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python@3.1.30",
      "config": { 
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "production/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}